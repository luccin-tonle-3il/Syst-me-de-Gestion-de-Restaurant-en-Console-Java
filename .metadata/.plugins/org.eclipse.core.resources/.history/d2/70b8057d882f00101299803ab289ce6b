package model;

public class Table {
	 private int id;
    private int numero;
	private String statut;
	public Table(int numero, String statut) {
		super();
		this.numero = numero;
		this.statut = statut;
	}
	public int getNumero() {
		return numero;
	}
	public void setNumero(int numero) {
		this.numero = numero;
	}
	public String getStatut() {
		return statut;
	}
	public void setStatut(String statut) {
		this.statut = statut;
	}
	
	
	    
}
